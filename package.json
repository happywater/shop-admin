    "@vitejs/plugin-vue": "^2.3.1",
    "eslint": "^8.13.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-vue": "^8.6.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "typescript": "^4.5.4",
    "vite": "^2.9.2",
    "vite-plugin-eslint": "^1.4.0",
